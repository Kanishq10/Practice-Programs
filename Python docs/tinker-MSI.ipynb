    "import tkinter as tk\n",
    "from tkinter import *\n",
    "win=Tk()\n",
    "win.geometry(\"300x350\")\n",
    "f1=Frame(win,bg=\"black\")\n",
    "f1.pack(fill=X)\n",
    "l1=Label(win,text=\"My Adder\",font=(\"arial\",12,\"bold\"))\n",
    "l1.pack()\n",
    "##layout manager\n",
    "win.mainloop()"
