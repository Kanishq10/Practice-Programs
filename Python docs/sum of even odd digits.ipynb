    "n=int(input())\n",
    "eve=0\n",
    "odd=0\n",
    "while n!=0:\n",
    "    if n%2==0:\n",
    "        eve+=n%10\n",
    "    else:\n",
    "        odd+=n%10\n",
    "    n=n//10\n",
    "print(eve,end=\" \")\n",
    "print(odd)\n"
